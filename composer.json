{
  "name": "myqaa/laravel-intl",
  "description": "Easy to use internationalization functions for Laravel",
  "keywords": [
    "laravel",
    "internationalization",
    "intl",
    "i18n",
    "l10n"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Propaganistas",
      "email": "Propaganistas@users.noreply.github.com"
    },
    {
      "name": "Eventdrive",
      "email": "contact@eventdrive.com"
    }
  ],
  "require": {
    "php": ">=7.1.3",
    "illuminate/events": "^8.0",
    "illuminate/support": "^8.0",
    "nesbot/carbon": "^2.0",
    "commerceguys/intl": "^1.0.5",
    "umpirsky/country-list": "^2.0",
    "umpirsky/locale-list": "^1.0",
    "punic/punic": "^3.5"
  },
  "require-dev": {
    "orchestra/testbench": "*",
    "phpunit/phpunit": "*"
  },
  "autoload": {
    "psr-4": {
      "Propaganistas\\LaravelIntl\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Propaganistas\\LaravelIntl\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Propaganistas\\LaravelIntl\\IntlServiceProvider"
      ]
    }
  }
}
